class Solution {
  public:
    int subarrayXor(vector<int>& arr) {
        // code here
        int n = arr.size();
        
        if((n & 1) == 0)
            return 0;
        
        int result = 0;
        
        for(int i = 0; i < n; i += 2) {
            result ^= arr[i];
        }
        
        return result;
    }
};

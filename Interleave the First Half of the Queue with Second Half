class Solution {
  public:
    void rearrangeQueue(queue<int> &q) {
        // code here
        int n = q.size();
        
        queue<int> firstHalf;
        for(int i = 0; i < n / 2; i++) {
            firstHalf.push(q.front());
            q.pop();
        }
        
        for(int i = 0; i < n / 2; i++) {
            q.push(firstHalf.front());
            firstHalf.pop();
            
            q.push(q.front());
            q.pop();
        }
        
        return;
    }
};

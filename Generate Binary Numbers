class Solution {
  public:
    vector<string> generateBinary(int n) {
        // code here
        queue<string> que;
        vector<string> result;
        
        que.push("1");
        
        for(int i = 0; i < n; i++) {
            string curr = que.front();
            que.pop();
            result.push_back(curr);
            
            que.push(curr + "0");
            que.push(curr + "1");
        }
        
        return result;
    }
};
